<Limit PUT DELETE>
order deny,allow
deny from all
</Limit>
#CRE_SEO Begin



Options +FollowSymLinks

RewriteEngine on




RewriteCond %{HTTP_HOST} ^pqbeauty.com/shop/$ [NC]
RewriteRule ^(.*)$ http://pqbeauty.com/$1 [R=301,L]
RewriteBase /shop/


#photo gallery

RewriteRule ^(.*)/cat-(.*)$ photo.php?catId=$2 [L]


#End photo gallery

#cre SEO check for images and skip all the other rules for performance reasons only
RewriteRule ^.*\.gif|\.jpg|\.png|\.css|\.js|\.php$ - [L]

# cre SEO reformats for product_info.html pages

# cre SEO reformats for product_info.html pages
RewriteRule ^m([0-9]+_?[0-9]*)/?(.*)/p([0-9]+_?[0-9]*)/(.*)$ product_info.php?manufacturers_id=$1&products_id=$3 [QSA,L]
RewriteRule ^p([0-9]+_?[0-9]*)/(.*)$ product_info.php?products_id=$1 [QSA,L]

#RewriteRule ^(.*)-c([^/]*)-p([0-9]+_?[0-9]*)-(.*)$ product_info.php?cPath=$2&products_id=$3 [QSA,L]

#presofttech2015 on 22092015
#RewriteRule ^(.*)/c([0-9]+_?[0-9]*)-p([0-9]+_?[0-9]*)$ product_info.php?cPath=$2&products_id=$3 [QSA,L]

RewriteRule ^(.*)/c(.*)-p([0-9]+_?[0-9]*)$ product_info.php?cPath=$2&products_id=$3 [QSA,L]
#presofttech2015 on 22092015

# cre SEO reformat for index.html pages
RewriteRule ^m(.*)/(.*)/p(.*)/(.*)/$ index.php?manufacturers_id=$1&products_id=$3 [QSA,L]
RewriteRule ^(.*)/m(.*)/(.*)$ index.php?manufacturers_id=$2 [QSA,L]
RewriteRule ^(.*)/c(.*)/m(.*)/(.*)/p(.*)/(.*)$ index.php?cPath=$2&products_id=$5 [QSA,L]
RewriteRule ^(.*)/c(.*)/p(.*)/(.*)$ index.php?cPath=$2&products_id=$3 [QSA,L]
#RewriteRule ^(.*)/c(.*)/(.*)$ index.php?cPath=$2 [QSA,L]

#RewriteRule ^(.+)/c([0-9]+)$ index.php?cPath=$2 [QSA,L]
#RewriteRule ^(.+)/c([0-9]+)_([0-9]+)$ index.php?cPath=$2 [QSA,L]

RewriteRule ^(.+)/c(.+)$ index.php?cPath=$2 [QSA,L]
RewriteRule index.html$ index.php [QSA,L]

# cre SEO reformat for information.html pages
RewriteRule ^i(.*)/(.*)/information$ information.php?info_id=$1 [QSA,L]

# cre SEO reformat for pages.html pages
#RewriteRule ^p(.*)/(.*)/pages\.html$ pages.php?pID=$1 [QSA,L]
#RewriteRule ^(.*)/c(.*)/p(.*)/(.*)/pages\.html$ pages.php?cID=$2&pID=$3 [QSA,L]
#RewriteRule ^(.*)/c(.*)/pages\.html$ pages.php?cID=$2 [QSA,L]
#RewriteRule ^(.*)/CDpath(.*)/p(.*)/(.*)/pages\.html$ pages.php?CDpath=$2&pID=$3 [QSA,L]
#RewriteRule ^(.*)/CDpath(.*)/pages\.html$ pages.php?CDpath=$2 [QSA,L]

#Presoftech2015 on 04/09/2015
RewriteRule ^(.*)/CD_([0-9]+)$ pages.php?CDpath=$2 [QSA,L]
RewriteRule ^(.*)/CD_([0-9]+)_([0-9]+)$ page.php?CDpath=$2_$3 [QSA,L]
#Presoftech2015 on 04/09/2015

#Presoftech2015

# cre SEO reformat for pages.html pages

#RewriteRule ^(.*)/pages.php$ pages.php?CDpath=$2 [QSA,L]

RewriteRule ^p(.*)/(.*)/pages\.html$ pages.php?pID=$1 [QSA,L]
RewriteRule ^(.*)/c(.*)/p(.*)/(.*)/pages\.html$ pages.php?cID=$2&pID=$3 [QSA,L]
RewriteRule ^(.*)/c(.*)/pages\.html$ pages.php?cID=$2 [QSA,L]
RewriteRule ^(.*)/CDpath(.*)/p(.*)/(.*)/pages\.html$ pages.php?CDpath=$2&pID=$3 [QSA,L]
RewriteRule ^(.*)/CDpath(.*)/pages\.html$ pages.php?CDpath=$2 [QSA,L]



#End Presoftech2015

# cre SEO reformat for articles.html pages
RewriteRule ^(.*)/t(.*)/a(.*)/(.*)$ articles.php?tPath=$2&articles_id=$3 [QSA,L]
RewriteRule ^(.*)/t(.*)$ articles.php?tPath=$2 [QSA,L]

# cre SEO reformat for article_info.html pages
RewriteRule ^a(.*)/(.*)$ article_info.php?articles_id=$1 [QSA,L]
RewriteRule ^(.*)/t(.*)$ article_info.php?tPath=$2 [QSA,L]

# cre SEO reformats for product_reviews.html pages
RewriteRule ^p(.*)/(.*)$ product_reviews.php?products_id=$1 [QSA,L]
RewriteRule ^(.*)/c(.*)/p(.*)/(.*)$ product_reviews.php?products_id=$3 [QSA,L]

# cre SEO reformats for product_reviews_info.html pages
RewriteRule ^p(.*)/(.*)/review(.*)$ product_reviews_info.php?products_id=$1&reviews_id=$3 [QSA,L]

# cre SEO reformat for FDMS pages
RewriteRule ^f(.*)/(.*)$ fdm_file_detail.php?file_id=$1 [QSA,L]
RewriteRule ^(.*)/fPath(.*)$ fdm_folder_files.php?fPath=$2 [QSA,L]

# cre SEO reformat for FSS pages
RewriteRule ^(.*)/fPath(.*)/form(.*)/(.*)$ fss_forms_detail.php?fPath=$2&forms_id=$3 [QSA,L]
RewriteRule ^(.*)/fPath(.*)$ fss_forms_index.php?fPath=$2 [QSA,L]


#cre SEO catch all in case other rules did not match
RewriteRule ^(product_info|index|information|pages|articles|article_info|product_reviews|product_reviews_info)\.html$ $1.php [QSA,L]
#CRE_SEO Install End


